{
  "name": "launcher-app",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "dev:renderer": "vite",
    "dev:electron": "wait-on tcp:5173 && electron .",
    "dev:backend": "cd server && npm run dev",
    "dev": "concurrently -k \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:full": "concurrently -k \"npm:dev:renderer\" \"npm:dev:backend\" \"npm:dev:electron\"",
    "build:renderer": "vite build",
    "build": "npm run build:renderer",
    "start": "electron .",
    "setup:backend": "cd server && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.2",
    "concurrently": "^9.2.1",
    "electron": "^37.4.0",
    "vite": "^7.1.4",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^17.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "socket.io-client": "^4.8.1"
  }
}
